# Test if/else/then conditionals

: test-if-else-true ( -- )
  5 3 >
  if
    "True branch" io.write-line
  else
    "False branch" io.write-line
  then
;

: test-if-else-false ( -- )
  3 5 >
  if
    "True branch" io.write-line
  else
    "False branch" io.write-line
  then
;

: test-equality ( -- )
  5 5 =
  if
    "Numbers are equal" io.write-line
  else
    "Numbers are not equal" io.write-line
  then
;

: main ( -- )
  test-if-else-true
  test-if-else-false
  test-equality
;
