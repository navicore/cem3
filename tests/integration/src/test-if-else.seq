# Test if/else/then conditionals

: test-if-else-true ( -- )
  # When condition is true, true branch executes
  5 3 >
  if
    1
  else
    0
  then
  1 test.assert-eq
;

: test-if-else-false ( -- )
  # When condition is false, else branch executes
  3 5 >
  if
    1
  else
    0
  then
  0 test.assert-eq
;

: test-equality ( -- )
  5 5 = test.assert
  5 3 = test.assert-not
;
