# Test comparison operators
# All comparisons return 1 (true) or 0 (false) as integers

: test-eq ( -- )
  5 5 = int->string write_line
  5 3 = int->string write_line
;

: test-lt ( -- )
  3 5 < int->string write_line
  5 3 < int->string write_line
;

: test-gt ( -- )
  5 3 > int->string write_line
  3 5 > int->string write_line
;

: test-lte ( -- )
  3 5 <= int->string write_line
  5 5 <= int->string write_line
  5 3 <= int->string write_line
;

: test-gte ( -- )
  5 3 >= int->string write_line
  5 5 >= int->string write_line
  3 5 >= int->string write_line
;

: test-neq ( -- )
  5 3 <> int->string write_line
  5 5 <> int->string write_line
;

: main ( -- )
  test-eq
  test-lt
  test-gt
  test-lte
  test-gte
  test-neq
;
