# Test comparison operators
# All comparisons return 1 (true) or 0 (false) as integers

: test-eq ( -- )
  5 5 = test.assert
  5 3 = test.assert-not
;

: test-lt ( -- )
  3 5 < test.assert
  5 3 < test.assert-not
;

: test-gt ( -- )
  5 3 > test.assert
  3 5 > test.assert-not
;

: test-lte ( -- )
  3 5 <= test.assert
  5 5 <= test.assert
  5 3 <= test.assert-not
;

: test-gte ( -- )
  5 3 >= test.assert
  5 5 >= test.assert
  3 5 >= test.assert-not
;

: test-neq ( -- )
  5 3 <> test.assert
  5 5 <> test.assert-not
;
