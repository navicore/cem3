# Test stack-utils.cem

: 2dup ( A B -- A B A B )
  2dup
;

: 2drop ( A B -- )
  drop drop
;

: 3drop ( A B C -- )
  3drop
;

: main ( -- )
  1 2 2dup
  2 = swap 1 = and nip nip
  if
    "2dup works ✓" io.write-line
  else
    "2dup FAILED" io.write-line
  then

  5 10 15
  2drop
  15 =
  if
    "2drop works ✓" io.write-line
  else
    "2drop FAILED" io.write-line
  then

  1 2 3 4
  3drop
  4 =
  if
    "3drop works ✓" io.write-line
  else
    "3drop FAILED" io.write-line
  then
;
